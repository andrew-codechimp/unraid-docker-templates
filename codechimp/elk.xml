<?xml version="1.0" encoding="utf-8"?>
<Containers>
<Beta>False</Beta>
<Category>Productivity: Tools:</Category>
  <Name>ELK</Name>
  <Description>
	Elasticsearch, Logstash, and Kibana
  [b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]
  [b]/config[/b] This is where the ELK applications will store their configuration files, database and logs.
  </Description>
  <Overview>
	Elastic provides products designed to search, analyze, and visualize your data, allowing you to get actionable insight in real time.
  </Overview>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/codechimporg/unraid-dockers-elk/</Registry>
  <Repository>codechimporg/unraid-dockers-elk</Repository>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9200</HostPort>
        <ContainerPort>9200</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>3333</HostPort>
        <ContainerPort>3333</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>3334</HostPort>
        <ContainerPort>3334</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>514</HostPort>
        <ContainerPort>514</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>      
      <Port>
        <HostPort>5601</HostPort>
        <ContainerPort>5601</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>    
  <Data>
    <Volume>
      <HostDir>path to config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:5601]/</WebUI>
  <Icon>https://raw.githubusercontent.com/codechimp-org/unraid-docker-templates/master/codechimp/images/elastic-icon.png</Icon>
</Containers>
